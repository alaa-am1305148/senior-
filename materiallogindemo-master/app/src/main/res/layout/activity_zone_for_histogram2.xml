<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/bacground1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.sourcey.materiallogindemo.zoneForHistogram2">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="92dp">

        <Button
            android:id="@+id/CENG_btn"
            android:layout_width="239dp"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="139dp"
            android:background="@drawable/rounded_button"
            android:onClick="buttonOnClick"
            android:padding="12dp"
            android:text="CENG Female Zone"
            android:textAllCaps="false"
            android:textColor="@android:color/white"
            android:textSize="18sp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="73dp"
            tools:layout_editor_absoluteY="106dp" />

        <Button
            android:id="@+id/CAAS_btn"
            android:layout_width="239dp"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/CENG_btn"
            android:layout_below="@+id/CENG_btn"
            android:layout_marginTop="34dp"
            android:background="@drawable/rounded_button"
            android:onClick="buttonOnClick"
            android:padding="12dp"
            android:text="CAAS Female Zone"
            android:textAllCaps="false"
            android:textColor="@android:color/white"
            android:textSize="18sp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="73dp"
            tools:layout_editor_absoluteY="199dp" />


    </RelativeLayout>


</android.support.constraint.ConstraintLayout>
